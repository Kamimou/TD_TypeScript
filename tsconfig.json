{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "moduleResolution": "node",
      "lib": ["ES2015", "DOM"],   
      "rootDir": "./td-typescript",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "verbatimModuleSyntax": false
    },
    "include": ["td-typescript/**/*.ts"]
  }
  